<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".registrar_automotor">

    <Button
        android:id="@+id/btnRAAceptar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="97dp"
        android:layout_marginTop="49dp"
        android:text="Cancelar"
        app:layout_constraintStart_toEndOf="@+id/btnRAAceptar"
        app:layout_constraintTop_toBottomOf="@+id/txtNumeroMatricula" />

    <EditText
        android:id="@+id/txtNumeroMatricula"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="23dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Número de Placa"
        app:layout_constraintStart_toEndOf="@+id/lbNumeroPlaca"
        app:layout_constraintTop_toBottomOf="@+id/txtNumeroChasis" />

    <TextView
        android:id="@+id/lbNumeroPlaca"
        android:layout_width="109dp"
        android:layout_height="19dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="49dp"
        android:layout_marginBottom="56dp"
        android:text="Número de Placa"
        app:layout_constraintBottom_toTopOf="@+id/btnRAAceptar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbNumeroChasis" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="430dp"
        android:layout_height="143dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logou" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/lbRegistrarAutomotor"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="261dp"
        android:text="Registrar Automotor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/lbNumeroChasis"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="27dp"
        android:text="Número de Chasis"
        app:layout_constraintBottom_toTopOf="@+id/lbNumeroPlaca"
        app:layout_constraintEnd_toStartOf="@+id/txtNumeroChasis"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbRegistrarAutomotor" />

    <EditText
        android:id="@+id/txtNumeroChasis"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="49dp"
        android:layout_marginBottom="23dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Número de Chasis"
        app:layout_constraintBottom_toTopOf="@+id/txtNumeroMatricula"
        app:layout_constraintStart_toEndOf="@+id/lbNumeroChasis"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/btnRAAceptar"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="97dp"
        android:text="Aceptar"
        app:layout_constraintEnd_toStartOf="@+id/btnRAAceptar2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbNumeroPlaca" />

</androidx.constraintlayout.widget.ConstraintLayout>