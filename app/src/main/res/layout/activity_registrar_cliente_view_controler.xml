        app:layout_constraintTop_toBottomOf="@+id/TxtEmailcl" />


    <EditText
        android:id="@+id/TxtFNacimientocl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TxtNCBancariacl" />

    <EditText
        android:id="@+id/TxtDireccioncl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="10"
