        app:layout_constraintTop_toBottomOf="@+id/TxtContrasenacl" />

    <EditText
        android:id="@+id/TxtNombrecl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/TxtApellidocl"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TxtCContrasenacl" />

    <EditText
        android:id="@+id/TxtApellidocl"
